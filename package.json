{
  "name": "Flappux",
  "version": "1.0.0",
  "description": "Challenge the two sides.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-packager . --all --out=dist Flappux",
    "build-darwin": "electron-packager . --out=dist/darwin --platform=darwin --arch=x64 Flappux && cp Icon.icns dist/darwin/Flappux-darwin-x64/Flappux.app/Contents/Resources/atom.icns",
    "build-win-32": "electron-packager . --out=dist/win --platform=win32 --arch=ia32 Flappux",
    "build-win-64": "electron-packager . --out=dist/win --platform=win32 --arch=x64 Flappux",
    "build-linux": "electron-packager .  --out=dist/linux --platform=linux --arch=x64 Flappux",
    "nixstart": "nix run nixpkgs.electron_6 -c electron .",
    "test": "return 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardacebi/flappux.git"
  },
  "author": "Arda Çebi",
  "contributors": [
    "Arda Çebi <ardacebi1@gmail.com> (https://www.ardacebi.com)",
    "Tan Siret Akıncı <tansiretakinci@yandex.com> (https://github.com/yutyo)"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ardacebi/flappux/issues"
  },
  "homepage": "https://github.com/ardacebi/flappux#readme",
  "devDependencies": {
    "electron": "^8.2.1",
    "electron-packager": "^14.2.1"
  },
  "launcher": {
    "title": "Flappux",
    "icon": "",
    "main": "./index.html",
    "width": 800,
    "height": 600
  }
}
